{
  "name": "@pepperse/ui",
  "version": "1.0.1",
  "repository": {
    "url": "https://github.com/nguyentran-se/peppers-ui",
    "directory": "nguyentran-se/peppers-ui"
  },
  "description": "Peppers UI",
  "main": "./cjs/index.js",
  "module": "./index.js",
  "types": "./index.d.ts",
  "files": [
    "dist"
  ],
  "scripts": {
    "test": "jest",
    "build:dev": "webpack --config webpack.dev.ts --watch",
    "build:prod": "webpack --config webpack.prod.ts",
    "build:test": "yarn build:prod && yarn gp",
    "gp": "ts-node scripts/dist-package.ts && prettier --write ./dist/package.json",
    "release": "yarn publish dist",
    "storybook": "start-storybook -p 6006",
    "build-storybook": "build-storybook",
    "prepare": "husky install",
    "format": "prettier --check ./src",
    "format:fix": "prettier --write ./src",
    "lint": "eslint ./src",
    "lint:fix": "eslint --fix ./src"
  },
  "keywords": [
    "react",
    "component"
  ],
  "author": "nguyentran-se",
  "license": "MIT",
  "devDependencies": {
    "@babel/core": "^7.18.13",
    "@babel/preset-env": "^7.18.10",
    "@babel/preset-react": "^7.18.6",
    "@babel/preset-typescript": "^7.18.6",
    "@commitlint/cli": "^17.1.2",
    "@commitlint/config-conventional": "^17.1.0",
    "@semantic-release/changelog": "^6.0.1",
    "@semantic-release/git": "^10.0.1",
    "@storybook/addon-actions": "^6.5.10",
    "@storybook/addon-essentials": "^6.5.10",
    "@storybook/addon-interactions": "^6.5.10",
    "@storybook/addon-links": "^6.5.10",
    "@storybook/builder-webpack5": "^6.5.10",
    "@storybook/manager-webpack5": "^6.5.10",
    "@storybook/preset-scss": "^1.0.3",
    "@storybook/react": "^6.5.10",
    "@storybook/testing-library": "^0.0.13",
    "@testing-library/react": "^13.4.0",
    "@types/jest": "^29.0.0",
    "@types/react": "^18.0.17",
    "@types/react-dom": "^18.0.6",
    "@typescript-eslint/eslint-plugin": "^5.33.0",
    "@typescript-eslint/parser": "^5.33.0",
    "babel-loader": "^8.2.5",
    "clsx": "^1.2.1",
    "css-loader": "^6.7.1",
    "eslint": "^8.23.0",
    "eslint-plugin-jsx-a11y": "^6.6.1",
    "eslint-plugin-react-hooks": "^4.6.0",
    "eslint-plugin-unused-imports": "^2.0.0",
    "filemanager-webpack-plugin": "^7.0.0",
    "fork-ts-checker-webpack-plugin": "^7.2.13",
    "husky": "^8.0.1",
    "jest": "^29.0.2",
    "lint-staged": "^13.0.3",
    "mini-css-extract-plugin": "^2.6.1",
    "prettier": "^2.7.1",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "react-icons": "^4.6.0",
    "sass": "^1.54.8",
    "sass-loader": "^13.0.2",
    "semantic-release": "^19.0.5",
    "style-loader": "^3.3.1",
    "ts-loader": "^9.3.1",
    "ts-node": "^10.9.1",
    "typescript": "^4.8.2",
    "webpack": "^5.74.0",
    "webpack-cli": "^4.10.0"
  },
  "lint-staged": {
    "*.{scss,css,json,md,html}": "yarn format:fix",
    "*.{ts,tsx}": [
      "yarn lint:fix",
      "yarn format:fix",
      "git add ."
    ]
  },
  "publishConfig": {
    "access": "public",
    "tag": "latest"
  },
  "dependencies": {}
}
